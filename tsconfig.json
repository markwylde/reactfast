{
   "compilerOptions": {
     "jsx": "react",
     "typeRoots": ["./node_modules/@types"],
     "strict": true,
     "target": "es2020",
     "module": "commonjs",
     "isolatedModules": true,
     "esModuleInterop": true,
     "importsNotUsedAsValues": "error",
     "noImplicitReturns": true,
     "forceConsistentCasingInFileNames": true,
     "useDefineForClassFields": true,
     "lib": ["es2020", "dom"]
   }
}
